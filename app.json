          "fonts": [
            "assets/fonts/Lato-Regular.ttf",
            "assets/fonts/Lato-Bold.ttf",
            "assets/fonts/Lato-Light.ttf",
            "assets/fonts/Heebo-Bold.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "dea10d5c-0894-4e62-98b2-ebc9639c1a11"
